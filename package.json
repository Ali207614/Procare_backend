    "express-rate-limit": "7.5.0",
    "helmet": "8.1.0",
    "ioredis": "5.6.1",
    "knex": "3.1.0",
    "nestjs-knex": "2.0.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.16.0",
    "rate-limit-redis": "4.2.1",
    "redis": "5.5.6",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "sanitize-html": "2.17.0",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "uuid": "11.1.0",
    "winston": "3.17.0"
