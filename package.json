    "@bull-board/express": "6.10.1",
    "@nestjs/bull": "11.0.2",
    "@nestjs/common": "11.1.3",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.3",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.3",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/websockets": "^11.1.3",
    "@sap/hana-client": "2.25.22",
    "axios": "1.9.0",
    "bcrypt": "6.0.0",
    "bull": "4.16.5",
